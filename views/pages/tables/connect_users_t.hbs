
<!-- Page Loader -->
<div class="page-loader-wrapper">
    <span class="loader"><span class="loader-inner"></span></span>
</div>

<div id="app">
<div class="main-wrapper main-wrapper-1">
    <div class="navbar-bg"></div>
{{>nav}}
{{>sidemenu}}
    


    <!-- Start app main Content -->
    <div class="main-content">
        <section class="section">
            <div class="section-header">
                <h1>Table</h1>
              
            </div>

            <div class="section-body">
                <h2 class="section-title">User Details</h2>
               

                <div class="row">
                    <div class="col-12 col-md-12 col-lg-12">
                        <div class="card">
                            <div class="card-header">
                                <h4>Users</h4>
                            </div>
                            <div class="card-body">

                 <table id="example" class="table table-striped table-bordered" style="width:100%">
             <thead>
            <tr>
                <th>Name</th>
                <th>Phone</th>
                <th>Email</th>
               <th>Location</th>
               <th>Date</th>
               <th>Actions</th>
            </tr>
        </thead>
        
        <tbody>
            {{#each users}}
                <tr>
                                        
                    <td>{{this.name}}</td>
                    <td>{{this.phoneNumber}}</td>
                    <td>{{this.email}}</td>
                    <td>{{this.surveyLocation}}</td>
                    <td>{{this.createdAt}}</td> 
                     <td>
                        <a href="/users/connect/edit/{{this._id}}"><button class="btn btn-success btn-sm rounded-0" type="button"  data-toggle="tooltip"  data-placement="top" title="Edit User"><i class="fa-solid fa-user-pen"></i></button></a>
                        <a href="/users/connect/delete/{{this._id}}"><button class="btn btn-danger btn-sm rounded-0" type="button"  data-toggle="tooltip"  data-placement="top" title="Delete User"><i class="fa-solid fa-trash"></i></button></a>
                    </td>
                </tr>
            {{/each}}
             </tbody>
        <tfoot>
            <tr>
                 <th>Name</th>
                  <th>Phone</th>
                   <th>Email</th>
               <th>Location</th>
                <th>Date</th>
                <th>Actions</th>
            </tr>
        </tfoot>
    </table>






